public class sort {
    static boolean issorted(int arr[]){
        for (int i = 0; i < arr.length; i++) {
            for (int j = i+1; j < arr.length; j++) {
                if(arr[j]<arr[i]){
                    return false;
                }
            }
        }
        return true;

    }
    public static void main(String[] args) {
        int[] arr = {15,10,7,20};
        boolean res = issorted(arr);
        System.out.println(res);
    }
    
}
